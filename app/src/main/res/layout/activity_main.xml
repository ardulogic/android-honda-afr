<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutCluster"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#0E0E0E"
        android:visibility="visible"
        tools:layout_editor_absoluteX="-185dp"
        tools:layout_editor_absoluteY="-42dp">

        <ImageView
            android:id="@+id/imageClusterGauge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/fuel_gauge_day" />


        <ImageView
            android:id="@+id/imageClusterLcd"
            android:layout_width="160dp"
            android:layout_height="67dp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.63"
            app:srcCompat="@drawable/fuel_gauge_lcd_day" />

        <TextView
            android:id="@+id/textClusterLcd"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/dsmonospace"
            android:gravity="right"
            android:text="000.1"
            android:textColor="#121212"
            android:textSize="40sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageClusterLcd"
            app:layout_constraintEnd_toEndOf="@+id/imageClusterLcd"
            app:layout_constraintStart_toStartOf="@+id/imageClusterLcd"
            app:layout_constraintTop_toTopOf="@+id/imageClusterLcd"
            app:layout_constraintVertical_bias="0.703" />


        <ImageView
            android:id="@+id/imageClusterRichFuel"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:foregroundGravity="center"
            android:foregroundTintMode="add"
            android:scaleType="fitXY"
            android:tint="#222222"
            android:paddingLeft="1dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageClusterNeedle"
            app:layout_constraintEnd_toEndOf="@+id/imageClusterNeedle"
            app:layout_constraintHorizontal_bias="0.697"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageClusterNeedle"
            app:layout_constraintVertical_bias="0.44"
            app:srcCompat="@drawable/fuel_gauge_rich_fuel" />

        <ImageView
            android:id="@+id/imageClusterGps"
            android:layout_width="28dp"
            android:layout_height="26dp"
            android:foregroundGravity="center"
            android:foregroundTintMode="add"
            android:scaleType="fitXY"
            android:tint="#5F9529"
            app:layout_constraintBottom_toBottomOf="@+id/imageClusterRichFuel"
            app:layout_constraintEnd_toEndOf="@+id/imageClusterNeedle"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageClusterRichFuel"
            app:layout_constraintVertical_bias="0.43"
            app:srcCompat="@drawable/gps" />


        <ImageView
            android:id="@+id/imageClusterObd"
            android:layout_width="40dp"
            android:layout_height="28dp"
            android:foregroundTintMode="add"
            android:tint="#222222"
            app:layout_constraintEnd_toEndOf="@+id/imageClusterRichFuel"
            app:layout_constraintStart_toStartOf="@+id/imageClusterRichFuel"
            app:layout_constraintTop_toBottomOf="@+id/imageClusterRichFuel"
            app:srcCompat="@drawable/obd" />


        <ImageView
            android:id="@+id/imageClusterAfr"
            android:layout_width="40dp"
            android:layout_height="28dp"
            android:adjustViewBounds="true"
            android:foregroundGravity="center"
            android:foregroundTintMode="add"
            android:scaleType="fitCenter"
            android:tint="#222222"
            app:layout_constraintEnd_toEndOf="@+id/imageClusterObd"
            app:layout_constraintStart_toStartOf="@+id/imageClusterObd"
            app:layout_constraintTop_toBottomOf="@+id/imageClusterObd"
            app:srcCompat="@drawable/afr" />

        <ImageView
            android:id="@+id/imageClusterNeedle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/fuel_gauge_needle_night" />

        <ImageButton
            android:id="@+id/buttonClusterTripKnob"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="@+id/imageClusterGauge"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.904"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageClusterGauge"
            app:layout_constraintVertical_bias="0.903"
            app:srcCompat="@drawable/trip_knob_day" />

        <TextView
            android:id="@+id/textClusterLcdMode1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/lcd_mode"
            android:layerType="software"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:text="TRIP"
            android:textColor="#222222"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="@+id/imageClusterLcd"
            app:layout_constraintTop_toTopOf="@+id/imageClusterLcd" />

        <TextView
            android:id="@+id/textClusterLcdMode2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="8dp"
            android:fontFamily="@font/lcd_mode"
            android:layerType="software"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:text="l/100"
            android:textColor="#222222"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/imageClusterLcd"
            app:layout_constraintTop_toTopOf="@+id/imageClusterLcd" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutScientific"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">


        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/graph"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayoutObd3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

        </com.github.mikephil.charting.charts.LineChart>

        <TextView
            android:id="@+id/textTargetAFR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-condensed"
            android:text="00.0"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="@+id/buttonIncreaseAFR"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/buttonIncreaseAFR" />


        <LinearLayout
            android:id="@+id/layoutTrip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:orientation="vertical"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout">

            <TextView
                android:id="@+id/textTotalInfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Total" />

            <TextView
                android:id="@+id/textOdometer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:foregroundGravity="center"
                android:gravity="center"
                android:text="00000.0"
                android:textSize="44sp" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="8dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textTotalLiters100km"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:foregroundGravity="center"
                    android:gravity="center"
                    android:text="0.00"
                    android:textSize="40sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:rotation="90"
                    android:text="100km"
                    android:textSize="10sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textTotalLiters"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:foregroundGravity="center"
                    android:gravity="center"
                    android:text="0.00"
                    android:textSize="34sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:rotation="90"
                    android:text="Total"
                    android:textSize="10sp" />
            </LinearLayout>

        </LinearLayout>

        <Button
            android:id="@+id/buttonTrackAfr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:backgroundTint="#353535"
            android:text="Track AFR"
            app:layout_constraintBottom_toBottomOf="@+id/graph"
            app:layout_constraintEnd_toEndOf="parent" />

        <LinearLayout
            android:id="@+id/layoutSound"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:orientation="vertical"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout">

            <TextView
                android:id="@+id/textSoundSmoothing"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:gravity="center"
                android:text="Engine Smoothing" />

            <SeekBar
                android:id="@+id/seekSmooth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp" />

            <TextView
                android:id="@+id/textSoundTps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Throttle" />

            <SeekBar
                android:id="@+id/seekTps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp" />

            <TextView
                android:id="@+id/textSoundRpm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="RPM" />

            <SeekBar
                android:id="@+id/seekRev"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="@+id/graph"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/buttonRecord"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#575757"
                android:text="Record" />

            <Space
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <com.hondaafr.Libs.UI.ImageButtonRounded
                android:id="@+id/buttonToggleShowGasConsumption"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:background="@drawable/image_button_on"
                android:contentDescription="Toggle Gas Consumption"
                android:padding="13dp"
                android:scaleType="centerInside"
                android:src="@drawable/gas"
                app:tint="#000" />

            <Space
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <com.hondaafr.Libs.UI.ImageButtonRounded
                android:id="@+id/buttonToggleSound"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:background="@drawable/image_button_on"
                android:contentDescription="Toggle Sound"
                android:padding="13dp"
                android:scaleType="centerInside"
                android:src="@drawable/sound"
                app:tint="#FFFFFF" />

            <Space
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <com.hondaafr.Libs.UI.ImageButtonRounded
                android:id="@+id/buttonToggleCluster"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:background="@drawable/image_button_on"
                android:contentDescription="Toggle Sound"
                android:padding="13dp"
                android:scaleType="centerInside"
                android:src="@drawable/speedometer"
                app:tint="#FFFFFF" />

            <Space
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/buttonClear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:backgroundTint="#606060"
                android:text="Clear" />

        </LinearLayout>


        <Button
            android:id="@+id/buttonIncreaseAFR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#6E23D8"
            android:text="+0.05 AFR"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.77" />

        <Button
            android:id="@+id/buttonDecreaseAFR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:backgroundTint="#6E23D8"
            android:text="-0.05 AFR"
            app:layout_constraintBottom_toBottomOf="@+id/textTargetAFR"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textTargetAFR"
            app:layout_constraintVertical_bias="0.5" />

        <com.google.android.material.button.MaterialButtonToggleGroup
            android:id="@+id/clearAfrBoundsGroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/textTargetAFR"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:singleSelection="true">

            <Button
                android:id="@+id/buttonClearAfrMin"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="00.0"
                android:textSize="20sp" />

            <Button
                android:id="@+id/buttonClearAfrAll"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="00.0"
                android:textSize="20sp" />

            <Button
                android:id="@+id/buttonClearAfrMax"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="00.0"
                android:textSize="20sp" />

        </com.google.android.material.button.MaterialButtonToggleGroup>

        <com.google.android.material.button.MaterialButtonToggleGroup
            android:id="@+id/toggleAfrGroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textTargetAFR"
            app:singleSelection="true">

            <Button
                android:id="@+id/buttonAfrSportPlus"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sport+" />

            <Button
                android:id="@+id/buttonAfrSport"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sport" />

            <Button
                android:id="@+id/buttonAfrEco"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Eco" />

            <Button
                android:id="@+id/buttonAfrEcoPlus"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Eco+" />
        </com.google.android.material.button.MaterialButtonToggleGroup>

        <TextView
            android:id="@+id/textMeasurementBig"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp"
            android:text="00.0"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/graph"
            app:layout_constraintStart_toStartOf="@+id/graph" />

        <TextView
            android:id="@+id/textMeasurementSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="00.0"
            android:textSize="34sp"
            app:layout_constraintBottom_toTopOf="@+id/textMeasurementBig"
            app:layout_constraintStart_toStartOf="@+id/textMeasurementBig" />

        <TextView
            android:id="@+id/textStatusObd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="6dp"
            android:text="Status (OBD): -"
            android:textColor="#A1A1A1"
            android:textSize="8sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textStatusSpartan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="6dp"
            android:text="Status (Spartan): -"
            android:textSize="8sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textStatusGeneric"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp"
            android:text="Status: -"
            android:textSize="8sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <LinearLayout
            android:id="@+id/linearLayoutConnectButtons"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/textStatusGeneric"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toggleAfrGroup">

            <Button
                android:id="@+id/buttonConnectObd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:backgroundTint="#595959"
                android:text="Connect OBD" />

            <Space
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:layout_weight="0" />

            <Button
                android:id="@+id/buttonConnectSpartan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:text="Connect Spartan" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutObd1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="64dp"
            android:layout_marginEnd="64dp"
            android:layout_marginBottom="16dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/clearAfrBoundsGroup"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <TextView
                android:id="@+id/textEct"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:padding="15dp"
                android:text="ECT" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textSpeed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.8"
                    android:paddingStart="15dp"
                    android:paddingLeft="15dp"
                    android:paddingTop="15dp"
                    android:paddingRight="15dp"
                    android:paddingBottom="5dp"
                    android:text="SPD" />

                <TextView
                    android:id="@+id/textSpeedSource"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2"
                    android:gravity="center"
                    android:text="OBD" />
            </LinearLayout>

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textStft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:padding="15dp"
                android:text="STFT" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutObd2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="64dp"
            android:layout_marginEnd="64dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/linearLayoutObd1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent">

            <TextView
                android:id="@+id/textRpm"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginBottom="0dp"
                android:padding="15dp"
                android:text="RPM" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textMap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:padding="15dp"
                android:text="MAP" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textLtft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:padding="15dp"
                android:text="LTFT" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayoutObd3"
